import java.util.Scanner;
public class stopWatch {
	static double CT = System.currentTimeMillis();
	private static double startTime = CT;
	
	public static double getST() {
		return startTime;
		
	}
	
	
	
	public static double Start() {
		return getST();
		
	}
	static double CT2 = System.nanoTime();
	private static double endTime = CT2;
	
	public static double getET() {
		return endTime;
	}
	public stopWatch() {
		getST();
	}
	public static double Stop() {
		return getET();
	}
	
	public static double getElapsedTime() {
		double elapsedTime = (getET() - getST());
		return elapsedTime;
	}
	
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		double C1 = System.currentTimeMillis(); 
